								}
							],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"dataflow": {
									"referenceName": "appendNewRows",
									"type": "DataFlowReference",
									"datasetParameters": {
										"sourceHistory": {
											"FileNameHistory": {
												"value": "@item().name",
												"type": "Expression"
											}
										},
										"sourceStaging": {
											"FileStaging": {
												"value": "@item().name",
												"type": "Expression"
											}
										},
										"writeAppendRows": {
											"FileNameHistory": {
												"value": "@item().name",
												"type": "Expression"
											}
										}
									}
								},
								"compute": {
									"coreCount": 8,
									"computeType": "General"
								},
								"traceLevel": "Fine"
							}
						}
					]
				}
